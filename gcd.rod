
func gcd(int a, int b) -> int { // gcd func
/*
    this function returns gcd of a and b
*/
    while (a && b) { // circle
        if (a > b) {
            a %= b;
        } else {
            b %= a;
        }
    } // end

/*
    now one of the values a or b is equal to gcd, and other is zero
*/
    return a + b; // so we can return a + b
    ++----++==*&%!dsao int 1232 0x123 0b1010101010 jfwoqi; // Ну пиздец ты насрал блин Лев
}

